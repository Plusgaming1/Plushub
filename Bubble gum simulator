-- Variables:

local plr = game.Players.LocalPlayer
local AutofarmOn = false
local CollectCoins = false

-- Functions: 

local function TeleportToIsland()
	local A_1 = "TeleportToCheckpoint"
	local A_2 = "The Floating Island"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	Event:FireServer(A_1, A_2)
end
local function ToSpawn()
	local A_1 = "TeleportToADoor"
	local A_2 = "The Floating Island"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	Event:FireServer(A_1, A_2)
end
local function GetObject()
	local least = math.huge
	local Object    
	for _,Stuff in pairs (workspace:GetDescendants()) do
		if Stuff:IsA("MeshPart") then
			if Stuff.MeshId == "rbxassetid://2542638609" or Stuff.MeshId == "rbxassetid://2542638609" or Stuff.MeshId == "rbxassetid://2558194071" or Stuff.MeshId == "rbxassetid://2558179804" then
				local Distance = game.Players.LocalPlayer:DistanceFromCharacter(Stuff.Position)
				if Distance < least and Distance < 100 then
					least = Distance
					Object = Stuff
				end
			end
		end
	end
	return Object
end
local function TeleportToSpace()
	local A_1 = "TeleportToCheckpoint"
	local A_2 = "Space"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	Event:FireServer(A_1, A_2)
end
local function Sell()
	local A_1 = "Teleport"
	local A_2 = "Sell"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	Event:FireServer(A_1, A_2)
end
local function BlowBubble()
	local A_1 = "BlowBubble"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	Event:FireServer(A_1)
end
local function TeleportToTwilight()
	local A_1 = "TeleportToCheckpoint"
	local A_2 = "The Twilight"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	Event:FireServer(A_1, A_2)
end
local function collectChest()
	local A_1 = "CollectChestReward"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	Event:FireServer(A_1, "The Skylands")
	Event:FireServer(A_1, "The Void")
	Event:FireServer(A_1, "The Floating Island")
end
local function TeleportToSkylands()
	local A_1 = "TeleportToCheckpoint"
	local A_2 = "The Skylands"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	Event:FireServer(A_1, A_2)
end
local function TeleportToVoid()
	local A_1 = "TeleportToCheckpoint"
	local A_2 = "The Void"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	Event:FireServer(A_1, A_2)
end
local function BuyAllEggs()
	local A_1 = "PurchaseEgg"
	local Event = game:GetService("ReplicatedStorage").NetworkRemoteEvent
	for i, egg in pairs(game.ReplicatedStorage.Assets.Eggs:GetChildren()) do
		Event:FireServer(A_1, egg.Name)
		wait()
	end
end
local function CollectAllCoinsAndGems()
	for i, coinorgem in pairs(workspace.Pickups:GetChildren()) do
		if coinorgem:IsA("Part") then
			for i = 1,10 do
				wait()
				plr.Character:MoveTo(coinorgem.Position)
			end
			wait(0.1)
		end
	end
end

