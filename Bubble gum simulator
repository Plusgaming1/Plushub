GemsAndCoins.TextWrapped = true
--[[ Btw if you remove this it wont work its very buggy
GemsAndCoins.MouseButton1Click:Connect(function()
	if CollectCoins then
		CollectCoins = false
		GemsAndCoins.Text = "Auto gems/coins: Off"
	else
		CollectCoins = true
		GemsAndCoins.Text = "Auto gems/coins: On"
		spawn(function()
			while wait() do
				if CollectCoins then
					local E = GetObject()
					if E ~= nil then
						plr.Character:MoveTo(E.Position)
					else
						CollectCoins = false
						GemsAndCoins.Text = "Auto gems/coins: Off"
						break
					end
				else
					break
				end
			end
		end)
	end
end)]]

